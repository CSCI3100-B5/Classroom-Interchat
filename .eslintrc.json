{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": ["warn", { "allow": "as-needed" }],
    "import/extensions": ["warn", "ignorePackages"],
    "react/prop-types": "off"
  }
}
